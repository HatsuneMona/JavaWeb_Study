<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="model.TeachersinfoEntity" table="teachersinfo" schema="javaweb">
    <id name="teacherNo" column="teacherNo"/>
    <property name="teacherName" column="teacherName"/>
    <property name="teacherAge" column="teacherAge"/>
    <property name="teacherSex" column="teacherSex"/>
    <property name="teacherDepartment" column="teacherDepartment"/>
  </class>
</hibernate-mapping>