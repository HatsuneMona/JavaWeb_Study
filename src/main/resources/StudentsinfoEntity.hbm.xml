<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="model.StudentsinfoEntity" table="studentsinfo" schema="javaweb">
    <id name="studentNo" column="studentNo"/>
    <property name="studentName" column="studentName"/>
    <property name="studentAge" column="studentAge"/>
    <property name="studentSex" column="studentSex"/>
  </class>
</hibernate-mapping>