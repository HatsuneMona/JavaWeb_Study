<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="model.CourseinfoEntity" table="courseinfo" schema="javaweb">
    <id name="courseNo" column="courseNo"/>
    <property name="courseName" column="courseName"/>
    <property name="firstClass" column="firstClass"/>
    <property name="courseCredit" column="courseCredit"/>
    <property name="teacher" column="teacher"/>
  </class>
</hibernate-mapping>